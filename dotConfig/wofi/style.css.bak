#window {
  background-color: rgba(131,51,52,0.5);
  box-shadow: 0 10px 15px -3px rgba(34,32,50,0.5), 0 4px 6px -4px rgba(34,32,50,0.5);
  font-size: 1.6rem;
  font-family: "JetBrains Mono NL";
}
#window #outer-box {
  padding: 0.15rem;
}
#window #outer-box #input {
  background-color: rgba(131,51,52,0.5);
  color: #DD585C;
  border: none;
  border-bottom: 1px solid rgba(34,32,50,0.5);
  padding: 1rem 1rem;
  font-size: 2.4rem;
}
#window #outer-box #input:focus, #window #outer-box #input:focus-visible, #window #outer-box #input:active {
  border: none;
  outline: 2px solid transparent;
  outline-offset: 2px;
}
#window #outer-box #scroll {
}
#window #outer-box #scroll #inner-box {
}
#window #outer-box #scroll #inner-box #entry {
  color: #142C44;
  background-color: rgba(221,88,92,0.5)
  padding: 1rem 1rem;
}
#window #outer-box #scroll #inner-box #entry #img {
  width: 1rem;
  margin-right: 1rem;
}
#window #outer-box #scroll #inner-box #entry:selected {
  color: #DD585C;
  background-color: rgba(221,88,92,0.5);
  outline: none;
}
